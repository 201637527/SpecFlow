{
  "version": "1.0.0-*",
  "buildOptions": {
    "outputName": "TechTalk.SpecFlow.Specs",
    "keyFile": "../../specflow.snk",
    "compile": {
      "excludeFiles": [
        "./Drivers/Templates/App.config",
        "./Drivers/Templates/BindingClass.cs",
        "./Drivers/Templates/BindingClass.fs",
        "./Drivers/Templates/Bindings.cs",
        "./Drivers/Templates/Bindings.fs",
        "./Drivers/Templates/TestProjectFile.csproj",
        "./Drivers/Templates/TestProjectFile.fsproj"
      ]
    },
    "embed": [
      "./Drivers/Templates/App.config",
      "./Drivers/Templates/BindingClass.cs",
      "./Drivers/Templates/BindingClass.fs",
      "./Drivers/Templates/Bindings.cs",
      "./Drivers/Templates/Bindings.fs",
      "./Drivers/Templates/TestProjectFile.csproj",
      "./Drivers/Templates/TestProjectFile.fsproj"
    ]
  },
  "dependencies": {
    "NUnit": "3.4.0",
    "dotnet-test-nunit": "3.4.0-beta-1",
    "NUnit.Extension.NUnitProjectLoader": "3.4.0",
    "NUnit.Extension.NUnitV2Driver": "3.4.0",

    "NUnit.Extension.VSProjectLoader": "3.4.0",
    "NUnit.Runners": "3.4.0",
    "FluentAssertions": "3.3.0",
    "Moq": "4.0.10827",
    "RhinoMocks": "3.6.1",
    "Gherkin": "4.0.0",
    "xunit": "1.9.2",
    "xunit.extensions": "1.9.2",
    "xunit.runner.console": "2.0.0",
    "TechTalk.SpecFlow.Parser": "1.0.0-*",
    "TechTalk.SpecFlow.Parser.Compatibility": "1.0.0-*",
    "TechTalk.SpecFlow.Reporting": {
        "version": "*",
        "target": "project"
    },
    "SpecFlow.NUnit3.ConsoleRunner": "1.0.0"
  },
  "tools": {
    "NUnit.Extension.NUnitV2ResultWriter": "3.2.1"

  },
  "testRunner": "nunit",
  "frameworks": {
    "net452": {
      "frameworkAssemblies": {
        "Microsoft.Build": "4.0.0.0",
        "Microsoft.Build.Framework": "4.0.0.0",
        "Microsoft.Build.Utilities.v4.0": "4.0.0.0"
      }
    }
  },
  "scripts": {
    "postcompile": "PostCompile.bat %project:Directory%"
  }
}
