<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="specFlow" type="TechTalk.SpecFlow.Configuration.ConfigurationSectionHandler, TechTalk.SpecFlow"/>
  </configSections>
  <appSettings>
    <add key="testProjectFolder" value="SpecFlowTestProject"/>
  </appSettings>
  <specFlow>
    <unitTestProvider name="xUnit" />
    <generator allowRowTests="false"/>
    
    <plugins>
      <add name="TechTalk.SpecFlow.Specs.Generator" type="Generator" path="..\TechTalk.SpecFlow.Specs.Generator.SpecFlowPlugin\bin\Debug\net471"/>
      <add name="TechTalk.SpecFlow.xUnit" type="Runtime" />
    </plugins>
  </specFlow>
</configuration>
